{
  "name": "eleventree",
  "version": "1.0.0",
  "description": "Linktree Alternative",
  "author": "Ted2xmen",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "npx @11ty/eleventy && tailwindcss -i _site/css/styles.sass -o dist/css/styles.css",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "tailwindcss -i _site/css/styles.sass -o dist/css/styles.css --watch --postcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ted2xmen/eleventree.git"
  },
  "homepage": "https://github.com/Ted2xmen/eleventree",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "autoprefixer": "^10.4.7",
    "dayjs": "^1.11.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@11ty/eleventy": "^1.0.1",
    "autoprefixer": "^10.4.7",
    "dayjs": "^1.11.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  }
}
